{
  "name": "ranchart-web-app",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "webpack:prod:build": "NODE_ENV=production webpack --config webpack.prod.js",
    "webpack:dev:build": "NODE_ENV=development webpack --config webpack.dev.js",
    "webpack:dev:watch": "NODE_ENV=development webpack --config webpack.dev.js --watch --color",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2022": true,
      "jest": true
    },
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "root": true,
    "rules": {
      "prettier/prettier": "warn",
      "import/extensions": "off",
      "import/no-unresolved": "off",
      "react-hooks/exhaustive-deps": "off",
      "react/jsx-indent": "error",
      "react/jsx-indent-props": "error",
      "react/jsx-boolean-value": "warn",
      "no-unused-vars": "warn",
      "no-underscore-dangle": "error",
      "no-param-reassign": "warn",
      "react/forbid-prop-types": "warn",
      "react/jsx-wrap-multilines": "off",
      "react/destructuring-assignment": "off",
      "no-console": "error",
      "jsx-a11y/label-has-associated-control": "off",
      "import/no-extraneous-dependencies": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".jsx",
            ".tsx"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-loader": "^9.1.2",
    "babel-preset-react-app": "^10.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.4",
    "echarts": "^5.4.2",
    "echarts-for-react": "^3.0.2",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^9.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "webpack": "^5.83.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^5.8.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
